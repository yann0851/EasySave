<Window x:Class="WpfAppEasySave.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppEasySave"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Button x:Name="Changement_de_langue" Content="Changement de langue" HorizontalAlignment="Left" Margin="61,102,0,0" VerticalAlignment="Top" Click="Ouvrir_Langue"/>
            <Button x:Name="Sauvegarde" Content="Sauvegarde" HorizontalAlignment="Left" Margin="61,10,0,0" VerticalAlignment="Top" Click="Ouvrir_Sauvegarde"/>
            <Button x:Name="Etat" Content="Etat" HorizontalAlignment="Left" Margin="61,11,0,0" VerticalAlignment="Top" Click="Ouvrir_Etat"/>
            <Button x:Name="Log" Content="Log" HorizontalAlignment="Left" Margin="61,12,0,0" VerticalAlignment="Top" Click="Ouvrir_Log"/>
            <Label Content="EasySave" HorizontalAlignment="Left" Margin="290,-210,0,0" VerticalAlignment="Top" FontSize="50"/>
        </StackPanel>
        <Frame x:Name="Main"/>
    </Grid>
</Window>
